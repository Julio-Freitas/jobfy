<!-- HEADER -->
<%- include('header') %>

<div class="container">
    <h2>Vagas de Emprego:</h2>
    <% categorias.forEach( categoria => {%> 
        
        <div class="content">
            <h3><%-categoria.categoria %></h3>
            <ul>
                <% categoria.vagas.forEach( vaga => { %>

                    <li><a href="vaga/<%-vaga.id %>"><%- vaga.titulo %></a></li>
                    
                <% })%>               
            </ul>
        </div>
    
    <% })%> 


</div> <!-- FIM CONTAINER-->


<!--  FOOTER -->
<%- include('footer') %>